{
  "name": "uni-flashswap-trader",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate-types": "npx typechain --target=truffle-v5 'build/contracts/*.json'",
    "post": "npm run compile && npm run generate-types",
    "lint": "solhint ./contracts/*.sol --fix && eslint --fix ./test/**",
    "prepare": "husky install",
    "prettier:solidity": "prettier --write ./contracts/**/*.sol",
    "compile": "npx truffle compile",
    "migrate": "npx truffle migrate",
    "migrate-ropsten": "npx truffle migrate --reset --network ropsten",
    "migrate-kovan": "npx truffle migrate --reset --network kovan",
    "migrate-mainnet": "npx truffle migrate --reset --network mainnet",
    "migrate-reset": "npx truffle migrate --reset",
    "console": "npx truffle console",
    "test": "npx truffle test",
    "console-ropsten": "truffle console --network ropsten",
    "console-kovan": "truffle console --network kovan",
    "console-mainnet": "truffle console --network mainnet",
    "build-deploy": "npm run compile && npm run migrate-reset",
    "build-deploy-ropsten": "npm run compile && npm run migrate-ropsten",
    "build-deploy-kovan": "npm run compile && npm run migrate-kovan",
    "build-deploy-mainnet": "npm run compile && npm run migrate-mainnet",
    "verify": "npx truffle run verify"
  },
  "keywords": [
    "flashloan",
    "trader",
    "bot"
  ],
  "author": "dmytro",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "3.4.2-solc-0.7",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.1",
    "truffle": "^5.5.17"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@truffle/hdwallet-provider": "^2.0.8",
    "@typechain/ethers-v5": "^8.0.4",
    "@typechain/truffle-v5": "^6.0.0",
    "@types/bignumber.js": "^5.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.4.1",
    "ethers": "^5.6.8",
    "husky": "^7.0.0",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "truffle-flattener": "^1.5.0",
    "truffle-plugin-verify": "^0.5.26",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.4",
    "typescript": "^4.5.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "web3": "^1.7.3"
  }
}
